{
  "name": "artist-website-svelte",
  "description": "Interactive svelte website showcasing the works of Eyewave.",
  "author": {
    "name": "_eyewave",
    "url": "https://github.com/eye-wave"
  },
  "homepage": "https://eyewave-website.vercel.app",
  "repository": "https://github.com/eye-wave/artist-website-svelte.git",
  "bugs": {
    "url": "https://github.com/eye-wave/artist-website-svelte/issues"
  },
  "version": "1.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "serve": "node file-server/server.cjs",
    "format": "prettier --plugin-search-dir . --write .",
    "eslint": "eslint . --fix",
    "lint": "npm run format && npm run eslint",
    "test": "vitest"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.79",
    "@poppanator/sveltekit-svg": "^3.0.1",
    "@sveltejs/adapter-vercel": "^3.0.1",
    "@sveltejs/kit": "^1.20.2",
    "@types/d3-scale": "^4.0.3",
    "@types/d3-shape": "^3.1.1",
    "@types/html-minifier": "^4.0.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.24",
    "postcss-nested": "^6.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-fast-marquee": "^0.4.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "unplugin-icons": "^0.16.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "html-minifier": "^4.0.0",
    "mongodb": "^5.6.0",
    "sharp": "^0.32.1"
  },
  "keywords": [
    "artist",
    "discography",
    "dubstep",
    "edm",
    "eyewave",
    "music",
    "svelte"
  ],
