{
  "private": true,
  "name": "eyewave-artist-website",
  "version": "1.0.0",
  "license": "MIT",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "add:workspace": "./scripts/add-workspace.cjs",
    "add:route": "./scripts/add-route.sh",
    "add:comp": "./scripts/add-comp.cjs",
    "add:store": "./scripts/add-store.cjs",
    "remove:workspace": "./scripts/remove-workspace.cjs",
    "remove:route": "./scripts/remove-route.sh",
    "update": "./scripts/update-deps.sh",
    "build": "yarn workspaces run build",
    "dev": "concurrently \"yarn workspace client run dev\" \"yarn workspace server run start\" \"yarn workspace server run dev\"",
    "preview": "concurrently \"yarn workspace client run preview\" \"yarn workspace server run start\"",
    "lint": "yarn workspaces run lint"
  },
  "devDependencies": {
    "concurrently": "*",
    "npm-check-updates": "*"
  }
}
